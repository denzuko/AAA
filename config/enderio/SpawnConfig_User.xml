  <entry id="ConcussionCreeper" mobName="enderiozoo:concussioncreeper" creatureType="MONSTER" rate="40"
    minGroupSize="1" maxGroupSize="3" remove="true">
    <biomeFilter type="any">
      <biome type="BASE_LAND_TYPES" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
      <biome type="MUSHROOM" exclude="true" />
    </biomeFilter>   
    <dimensionExclude name="twilight_forest"/><!-- sic! TF uses its id for human readable name, too -->
    <dimensionExclude name="Dungeon"/>
  </entry>

  <entry id="EnderMiny" mobName="enderiozoo:enderminy" rate="10">
    <biomeFilter type="any" />
  </entry>

  <entry id="FallenKnight" mobName="enderiozoo:fallenknight" rate="30" minGroupSize="1" maxGroupSize="4" remove="true">
    <biomeFilter type="any">
      <biome type="PLAINS" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
    <dimensionExclude name="twilight_forest"/><!-- sic! TF uses its id for human readable name, too -->
  </entry>

  <entry id="WitherWitch" mobName="enderiozoo:witherwitch" rate="5" minGroupSize="1" maxGroupSize="1" remove="true">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="SWAMP" />
      <biome type="PLAINS" />
      <biome type="HILLS" />
      <biome type="MOUNTAIN" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
    <dimensionExclude name="twilight_forest"/><!-- sic! TF uses its id for human readable name, too -->
  </entry>

  <entry id="DireWolf" mobName="enderiozoo:direwolf" rate="30" minGroupSize="2" maxGroupSize="6" remove="true">
    <biomeFilter type="all">
      <biome type="SNOWY" />
      <biome type="CONIFEROUS" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
  </entry>

   <entry id="Owl" mobName="enderiozoo:owl" rate="5" minGroupSize="1" maxGroupSize="1" creatureType="CREATURE">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="RIVER" />
      <biome type="WASTELAND" exclude="true" />
      <biome type="JUNGLE" exclude="true" />
      <biome type="SNOWY" exclude="true" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
    </biomeFilter>
  </entry>

  <entry id="LoveChild" mobName="enderiozoo:lovechild" creatureType="MONSTER" rate="10" minGroupSize="1" maxGroupSize="1" remove="true">
    <biomeFilter type="any">
      <biome type="BASE_LAND_TYPES" />
      <biome type="NETHER" exclude="true" />
      <biome type="END" exclude="true" />
      <biome type="MUSHROOM" exclude="true" />
    </biomeFilter>
    <dimensionExclude/>
    <dimensionInclude name="overworld"/>
  </entry>

  <entry id="EpicSquid" mobName="enderiozoo:epicsquid" creatureType="WATER_CREATURE" rate="1" minGroupSize="1" maxGroupSize="1" remove="false">
    <biomeFilter type="any" />
    <dimensionExclude/>
    <dimensionInclude name="overworld"/>
    <dimensionInclude name="the_nether"/>
  </entry>
</SpawnConfig>

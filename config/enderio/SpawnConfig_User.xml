   <entry id="WitherWitch" mobName="enderiozoo:witherwitch" rate="20" minGroupSize="1" maxGroupSize="1">
    <biomeFilter type="any">
      <biome type="FOREST" />
      <biome type="SWAMP" />
      <biome type="PLAINS" />
      <biome type="HILLS" />
      <biome type="MOUNTAIN" />
      <biome type="NETHER" exclude="true" />
